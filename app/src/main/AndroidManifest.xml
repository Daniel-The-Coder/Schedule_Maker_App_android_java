<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hfad.schedule_maker"
    android:versionCode="2"
    android:versionName="2.01">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".scheduleIntermediate"
            android:label="@string/title_activity_schedule_intermediate"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ViewSchedules"
            android:label="@string/title_activity_view_schedules"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
